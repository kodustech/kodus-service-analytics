{
  "name": "kodus-analytics-service",
  "version": "0.0.1",
  "description": "Analytics service with BigQuery integration",
  "main": "dist/index.js",
  "license": "MIT",
  "resolutions": {
    "jws": "^4.0.1",
    "validator": "^13.15.0",
    "js-yaml": "^4.1.1",
    "brace-expansion": "^2.0.2"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only --inspect=0.0.0.0:9230 ./src/index.ts",
    "build": "yarn tsc --build",
    "serve": "yarn build && yarn start",
    "test": "jest",
    "docker:build:dev": "docker-compose -f docker-compose.dev.yml build",
    "docker:build:qa": "docker-compose -f docker-compose.qa.yml build",
    "docker:build:prod": "docker-compose -f docker-compose.prod.yml build",
    "docker:start": "yarn docker:down:dev && yarn docker:up:dev",
    "docker:up:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:down:dev": "docker-compose -f docker-compose.dev.yml down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^8.1.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "node-cache": "^5.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}