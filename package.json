{
  "name": "kodus-analytics-service",
  "version": "0.0.1",
  "description": "Analytics service with BigQuery integration",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only --inspect=0.0.0.0:9230 ./src/index.ts",
    "build": "yarn tsc --build",
    "serve": "yarn build && yarn start",
    "test": "jest",
    "docker:build:dev": "docker-compose -f docker-compose.dev.yml build",
    "docker:build:qa": "docker-compose -f docker-compose.qa.yml build",
    "docker:build:prod": "docker-compose -f docker-compose.prod.yml build",
    "docker:start": "yarn docker:down:dev && yarn docker:up:dev",
    "docker:up:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:down:dev": "docker-compose -f docker-compose.dev.yml down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-cache": "^5.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.13.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
